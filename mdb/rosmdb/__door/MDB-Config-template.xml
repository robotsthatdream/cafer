<?xml version="1.0" encoding="UTF-8"?>

<!--
Configuración del MDB para probar el Simulador en el que el robot debe alcanzar la ficha (o pelota) roja.
En el simulador se configura un solo sensor que envía información sobre el ángulo, distancia y si está visible o no la pelota.

La selección de acciones se realiza directamente desde el principio con los comportamientos

-->
<MDB>
    <core>
        <stopCondition>
            <class>es.udc.gii.mdb.core.condition.MaxIterationsStopCondition</class>
            <maxIterations>20000</maxIterations>          
        </stopCondition>
        <ip>localhost</ip>
         <strategyChooser>
            <id>exploratoryChooser</id>
            <class>es.udc.gii.mdb.action.chooser.exploration.CollectTheBallWithDoorExplorationChooser</class>

           
            <explorationCriteria> <!-- used for ExplorationChooser -->
                <class>es.udc.gii.mdb.action.chooser.exploration.evo.EvoExplorationCriteria</class>
                <useAction>false</useAction>
                <n>5</n>
            </explorationCriteria>




            <candidateActionSelector> <!-- used for ExplorationChooser -->
                <class>es.udc.gii.mdb.action.chooser.exploration.RandomActionSelector</class>
                <numberOfActions>32</numberOfActions>
                <actuator>motor</actuator>
            </candidateActionSelector>

            
            <logs>
                    <log>
                        <class>es.udc.gii.mdb.util.log.ExplorationCriteriaLogTool</class>
                        <folder>__door/_log</folder>
                        <iterations>1</iterations>
                    </log>  
                    <log>
                        <class>es.udc.gii.mdb.util.log.ExplorationBestCriteriaLogTool</class>
                        <folder>__door/_log</folder>
                        <iterations>1</iterations>
                    </log>   
                 <!--   <log>
                        <class>es.udc.gii.mdb.util.log.DensityCertaintyPostAntiTraceAproxLogTool</class>
                        <folder>__door/_log</folder>
                        <iterations>1</iterations>
                    </log>   
-->

            </logs>

        </strategyChooser>
        <accomplished>accomplished</accomplished>
        <savePath>__door/_log</savePath>
        <logTool>
            <log>
                <class>es.udc.gii.mdb.util.log.MDBGoalAccomplishedLogTool</class>
                <folder>__door/_log</folder>
                <iterations>1</iterations>
            </log>
            <log>
                <class>es.udc.gii.mdb.util.log.MDBRewardLogTool</class>
                <folder>__door/_log</folder>
                <iterations>1</iterations>
            </log>
            <log>
                <class>es.udc.gii.mdb.util.log.MDBAPPLogTool</class>
                <folder>__door/_log</folder>
                <iterations>1</iterations>
            </log>            
           
        </logTool>
        <stopLearning>1000000</stopLearning>
        <saveIterations>10000</saveIterations>
        <iterationToSubgoal>50000</iterationToSubgoal>
        <episodicBufferSize>200</episodicBufferSize>
        <maxSubgoals>_MAXSG_</maxSubgoals>

    </core>

